</head>
<body>
    {{>navbar}}
     <div class="test">
        <div class="box-drop dis">
            <a href="/cart">Your Cart</a>
            <a href="#">Wishlist</a>
            <a href="#">Checkout</a>
        </div>
    </div>

    <div class="test1">
        <div class="box-drop dis">
            <h6>Welcome, {{name}}</h6>
            <a href="/acc">Your Account</a>
            <a href="#">Your Orders</a>
            <a href="/logout" class="logging-out">Log out</a>
        </div>
    </div>
    <div class="main-heading">
        <h1>Your Orders</h1>
    </div>
    <div class="line"></div>

    <div class="container-fluid my-5 d-sm-flex justify-content-center">
        <div class="card px-2">
            <div class="card-header bg-white">
                <div class="row justify-content-between">
                    <div class="col">
                        <p class="text-muted"> Order ID <span class="font-weight-bold text-dark">1222528743</span></p>
                        <p class="text-muted"> Place On <span class="font-weight-bold text-dark">26,March 2022</span> </p>
                    </div>
                    <div class="flex-col my-auto">
                        <h6 class="ml-auto mr-3"> <a href="#">View Details</a> </h6>
                    </div>
                </div>
            </div>
            <div class="card-body">
                <div class="media flex-column flex-sm-row">
                    <div class="media-body ">
                        <h5 class="bold">Realme TechLife</h5>
                        <p class="text-muted"> Qt: 1</p>
                        <h4 class="mt-3 mb-4 bold"> <span class="mt-5">&#x20B9;</span> 5,000 <span class="small text-muted"> via (COD) </span></h4>
                        <p class="text-muted">Tracking Status on: <span class="Today">11:30pm, Today</span></p> <button type="button" class="btn btn-outline-primary d-flex">SHIPPED</button>
                    </div><img class="align-self-center img-fluid" src="./Images/realme TechLife.png">
                </div>
            </div>
            <div class="row px-3">
                <div class="col">
                    <ul id="progressbar">
                        <li class="step0 active " id="step1">PLACED</li>
                        <li class="step0 active text-center" id="step2">SHIPPED</li>
                        <li class="step0 text-muted text-right" id="step3">DELIVERED</li>
                    </ul>
                </div>
            </div>
            <div class="card-footer bg-white px-sm-3 pt-sm-4 px-0">
                <div class="row text-center ">
                    <div class="col my-auto border-line ">
                        <h5>Track</h5>
                    </div>
                    <div class="col my-auto border-line ">
                        <h5>Cancel</h5>
                    </div>
                    <div class="col my-auto mx-0 px-0 ">
                        <h5>Pre-pay</h5>
                    </div>
                </div>
            </div>
        </div>
    </div>
