    <div class="pro_price">
      <div class="price_num_discount">₹74,000</div>
      <div class="price_num_actual"><del class="crosspri">₹93,690</del>
        <span class="pri_per_discount">21% off</span>
      </div>
      <div class="delivery">Free delivery</div>
    </div>
  </div>
</div>
<div class="border-line"></div> 

